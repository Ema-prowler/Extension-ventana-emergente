{
    "manifest_version": 3,
    "name": "Ventana Emergente",
    "version": "0.1",
    "description": "Crea una ventana emergente que hace algo",
    "action": {
    "default_icon": "/assets/Icon.png",
    "default_popup": "popup.html"
    },
    "permissions": [
      "activeTab",
      "storage"
    ],
    "host_permissions": [
      "https://www.youtube.com/"
    ],
    "background":{
      "service_worker": "background.js"
    }
  }